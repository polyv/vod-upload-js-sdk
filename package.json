    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.2.0",
    "@babel/runtime-corejs2": "^7.2.0",
    "ali-oss": "^6.1.0",
    "autoprefixer": "^9.4.2",
